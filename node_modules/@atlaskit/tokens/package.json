{
  "name": "@atlaskit/tokens",
  "version": "1.2.9",
  "author": "Atlassian Pty Ltd",
  "description": "Design tokens are the single source of truth to name and store design decisions.",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "atlassian": {
    "team": "Design System Team",
    "releaseModel": "continuous",
    "website": {
      "name": "Design tokens",
      "category": "Libraries"
    }
  },
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    ">=4.0 <4.5": {
      "*": [
        "dist/types-ts4.0/*"
      ]
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "atlaskit:src": "src/index.tsx",
  "af:exports": {
    ".": "./src/index.tsx",
    "./figma/atlassian-light.json": "./figma/atlassian-light.json",
    "./figma/atlassian-dark.json": "./figma/atlassian-dark.json",
    "./palettes-raw": "./src/entry-points/palettes-raw.tsx",
    "./tokens-raw": "./src/entry-points/tokens-raw.tsx",
    "./token-ids": "./src/entry-points/token-ids.tsx",
    "./token-names": "./src/entry-points/token-names.tsx",
    "./token-order": "./src/entry-points/token-order.tsx",
    "./rename-mapping": "./src/entry-points/rename-mapping.tsx",
    "./babel-plugin": "./src/entry-points/babel-plugin.tsx"
  },
  "scripts": {
    "ak-postbuild": "(cd $(npx repo-root) && yarn build @af/codegen) && yarn workspace @atlaskit/tokens codegen-tokens && yarn workspace @atlaskit/tokens check-clean-git",
    "check-clean-git": "git diff --exit-code -- packages/design-system/tokens/ || (echo 'tokens are out of date, run yarn build tokens' && false)",
    "codegen-tokens": "ts-node --project $(npx repo-root)/tsconfig.node.json ./scripts/style-dictionary/build"
  },
  "dependencies": {
    "@atlaskit/ds-lib": "^2.1.0",
    "@atlaskit/platform-feature-flags": "^0.1.0",
    "@babel/runtime": "^7.0.0",
    "@babel/traverse": "^7.15.0",
    "@babel/types": "^7.15.0",
    "bind-event-listener": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@af/accessibility-testing": "*",
    "@af/codegen": "*",
    "@atlaskit/avatar": "^21.2.0",
    "@atlaskit/badge": "^15.0.11",
    "@atlaskit/button": "^16.6.0",
    "@atlaskit/checkbox": "^12.4.0",
    "@atlaskit/code": "^14.5.0",
    "@atlaskit/docs": "^9.0.10",
    "@atlaskit/dropdown-menu": "^11.7.0",
    "@atlaskit/ds-explorations": "^2.0.0",
    "@atlaskit/dynamic-table": "^14.8.0",
    "@atlaskit/empty-state": "^7.4.0",
    "@atlaskit/focus-ring": "^1.2.0",
    "@atlaskit/grid": "^0.8.0",
    "@atlaskit/heading": "^1.2.0",
    "@atlaskit/icon": "^21.11.0",
    "@atlaskit/lozenge": "^11.3.0",
    "@atlaskit/modal-dialog": "^12.4.0",
    "@atlaskit/primitives": "^0.6.0",
    "@atlaskit/section-message": "^6.3.0",
    "@atlaskit/tabs": "^13.3.0",
    "@atlaskit/tag": "^11.4.0",
    "@atlaskit/textfield": "^5.3.0",
    "@atlaskit/theme": "^12.3.0",
    "@atlaskit/toggle": "^12.5.0",
    "@atlaskit/tooltip": "^17.7.0",
    "@atlassian/atlassian-frontend-prettier-config-1.0.1": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.1",
    "@babel/core": "^7.12.3",
    "@emotion/core": "^10.0.9",
    "@emotion/react": "^11.7.1",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/chrome": "^0.0.171",
    "copy-webpack-plugin": "^6.4.0",
    "echarts": "^5.4.1",
    "echarts-for-react": "^3.0.2",
    "fuse.js": "^6.6.2",
    "lodash": "^4.17.21",
    "prettier": "^2.7.0",
    "react": "^16.8.0",
    "react-use-clipboard": "^1.0.0",
    "recharts": "^1.8.5",
    "style-dictionary": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "4.5.5"
  },
  "techstack": {
    "@atlassian/frontend": {
      "import-structure": "atlassian-conventions",
      "circular-dependencies": "file-and-folder-level"
    },
    "@repo/internal": {
      "dom-events": "use-bind-event-listener",
      "design-system": "v1",
      "ui-components": "lite-mode",
      "deprecation": "no-deprecated-imports",
      "styling": [
        "emotion"
      ]
    }
  },
  "platform-feature-flags": {
    "design-system-team.dark-theme-iteration_dk1ln": {
      "type": "boolean"
    }
  },
  "homepage": "https://atlassian.design/components/tokens",
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.1"
}
